<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".regular_next">


    <TextView
        android:id="@+id/textView6"
        android:layout_width="348dp"
        android:layout_height="22dp"
        android:layout_marginTop="24dp"
        android:text="@string/r2"
        android:textColor="@android:color/secondary_text_light"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        tools:text="@string/r2" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="348dp"
        android:layout_height="22dp"
        android:layout_marginTop="24dp"
        android:text="@string/r3"
        android:textColor="@android:color/secondary_text_light"
        app:layout_constraintEnd_toEndOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        tools:text="@string/r3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="347dp"
        android:layout_height="23dp"
        android:layout_marginTop="148dp"
        android:text="@string/r1"
        android:textColor="@android:color/secondary_text_light"
        app:layout_constraintEnd_toEndOf="@+id/imageN"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageN"
        app:layout_constraintTop_toBottomOf="@+id/imageCN"
        tools:text="@string/r1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="348dp"
        android:layout_height="22dp"
        android:text="@string/r4"
        android:textColor="@android:color/secondary_text_light"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/ChelN"
        tools:text="@string/r4" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="358dp"
        android:layout_height="81dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView12"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:srcCompat="@drawable/mode_1_default" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="334dp"
        android:layout_height="89dp"
        android:layout_marginBottom="108dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.311"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/s12" />

    <ImageView
        android:id="@+id/imageCN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/se5" />

    <Button
        android:id="@+id/backr2"
        android:layout_width="39dp"
        android:layout_height="50dp"
        android:layout_marginBottom="2dp"
        android:background="?attr/selectableItemBackground"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/imageCN"
        app:layout_constraintEnd_toEndOf="@+id/imageCN"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageCN"
        app:layout_constraintTop_toTopOf="@+id/imageCN"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageN"
        android:layout_width="349dp"
        android:layout_height="137dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/imageCN"
        app:layout_constraintTop_toBottomOf="@+id/imageCN"
        app:layout_constraintVertical_bias="0.259"
        app:srcCompat="@drawable/cover" />

    <ImageView
        android:id="@+id/imageCN1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:layout_constraintVertical_bias="0.6"
        app:srcCompat="@drawable/create" />

    <Button
        android:id="@+id/createOpr2"
        android:layout_width="368dp"
        android:layout_height="58dp"
        android:background="?attr/selectableItemBackground"
        app:layout_constraintBottom_toBottomOf="@+id/imageCN1"
        app:layout_constraintStart_toStartOf="@+id/imageCN1"
        app:layout_constraintTop_toTopOf="@+id/imageCN1" />

    <TextView
        android:id="@+id/nameCborN"
        android:layout_width="347dp"
        android:layout_height="20dp"
        android:layout_marginTop="4dp"
        android:text="TextView"
        android:textColor="@android:color/secondary_text_light"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/MoneyN"
        android:layout_width="347dp"
        android:layout_height="20dp"
        android:text="TextView"
        android:textColor="@android:color/secondary_text_light"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/ChelN"
        android:layout_width="347dp"
        android:layout_height="20dp"
        android:text="TextView"
        android:textColor="@android:color/secondary_text_light"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/descN"
        android:layout_width="347dp"
        android:layout_height="20dp"
        android:text="TextView"
        android:textColor="@android:color/secondary_text_light"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/imageView11"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>